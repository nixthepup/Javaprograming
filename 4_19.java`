default:
JOptionPane.showMessageDialog(null,"The number " + colorIndex + " is not matched with a color!");
break;
}
userguessedColor = JOptionPane.showInputDialog("I have picked a color( Red,Green,Blue,Organe,Yellow) guess what it is:");

